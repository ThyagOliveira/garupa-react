{
  "extends": [
    "eslint:recommended", 
    "react-app",
    "plugin:prettier/recommended"
  ],
  "plugins": ["prettier", "react-hooks"],
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": "error",    
    "react-hooks/rules-of-hooks": "error",
    "no-console": "off"
  }
}
